{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6", "ScriptHost"],
    "module": "CommonJS",
    "declaration": true,
    "moduleResolution": "Node",
    "removeComments": true,
    "strict": true,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
